{
  "name": "todo-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "start:todo": "concurrently \"nx serve todo-app\" \"npm run start:todo-server\"",
    "start:todo-server": "nx serve todo-service",
    "build": "nx build",
    "build:prod": "ng build --prod",
    "build:ui-elements": "nx build shared-ui --output-hashing=none",
    "start:pwa": "rimraf dist && npm run build && cd dist && http-server -p 4200",
    "test": "nx test",
    "test:pr": "npm run affected:test -- --base origin/master --head HEAD",
    "test:watch": "ng test --watch true --browsers=Chrome",
    "lint": "./node_modules/.bin/nx lint && nx lint",
    "e2e": "nx e2e todo-app-e2e",
    "codecov": "codecov -t ",
    "start:reactapp": "npm run build -- shared-ui && npm run start -- reactapp",
    "start:reactapp:watch": "onchange -i -k 'libs/shared/ui/**/*.*' -- npm run start:reactapp",
    "e2e:todo-app": "start-server-and-test start:todo-server http-get://localhost:3333/api/todo-list 'npm run e2e -- todo-app-e2e'",
    "e2e:todo-app:watch": "start-server-and-test start:todo-server http-get://localhost:3333/api/todo-list 'npm run e2e -- todo-app-e2e --watch'",
    "e2e:todo-admin": "start-server-and-test start:todo-server http-get://localhost:3333/api/todo-list 'npm run e2e -- todo-admin-e2e'",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build --parallel",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:e2e:pr": "start-server-and-test start:todo-server http-get://localhost:3333/api/todo-list 'npm run affected:e2e -- --base origin/master --head HEAD --headless'",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint --parallel",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "help": "./node_modules/.bin/nx help",
    "start:storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o dist/design-system"
  },
