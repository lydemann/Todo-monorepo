						"tsConfig": "libs/shared/data-access/tsconfig.spec.json",
						"karmaConfig": "libs/shared/data-access/karma.conf.js"
					}
				}
			},
			"schematics": {
				"@nrwl/schematics:component": {
					"styleext": "scss"
				}
			}
		},
		"shared-util": {
			"root": "libs/shared/util",
			"sourceRoot": "libs/shared/util/src",
			"projectType": "library",
			"prefix": "todo",
			"architect": {
